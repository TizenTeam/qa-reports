- testcase = testcase_build
tr.testcase
  td.testcase_name= testcase.name
  - @build.reverse_each do |h|
    - tc = testcase.find_matching_case(h)
    - if tc
      td class="testcase_result build_result #{result_class(tc)}" #{result_html(tc)}
    - else
      td class="testcase_result build_result" -
  td class="testcase_result #{result_class(testcase)} build_result" #{result_html(testcase)}
  td
