              <th class="th_name">Measurement</th>
              <th class="th_measured">Value</th>
              <th class="th_target">Target</th>    
              <th class="th_limit">Fail limit</th>	
              <th class="th_to_target">% of target</th>  
              <th class="th_result">Result</th>  			     			  
              <th id="th_notes">Notes</th>
            </tr>
          </thead>
          
				<!-- ***** DUMMY HTML FOR NOTE ATTACHEMNTS STARTS ******** -->          
					<tbody>
						<tr class="feature_name" id="test-set-370">
							<td colspan="8">
						        N/A
								<a href="#" class="see_all_toggle" style="display: none; ">- hide passing tests</a>
							</td>
						</tr>
					</tbody>
					<tbody>
					<tr class="testcase multiple_rows odd" id="testcase-823">
					
					  <td class="testcase_name" rowspan="60">DUMMY HTML ONLY, remove ****************
					    
					    
					  </td>
					    <td class="testcase_measurement">GPS fixes</td>
					    <td class="testcase_value"></td>
					    <td class="testcase_target">80 <span class="unit">%</span></td>   
					    <td class="testcase_limit">75 <span class="unit">%</span></td>    
