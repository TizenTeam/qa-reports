					      <td class="testcase_result multiple_results pass   " rowspan="60"><span class="content">Pass</span> </td>       			
					      <td class="testcase_notes " rowspan="60">
					        <div class="content">
					        	<a id="open_note_attachment_dialog" class="note_attachment">Open attachment</a>    
					        	
					        </div>
					      </td>
							</tr>
					</tbody>
  	<!-- ************* DUMMY HTML ENDS ******************* -->


          <%= render :partial => "nft_feature", :collection => @test_session.nft_sets %>

        </table>

      <% end %>


      <%= render :partial => 'editable_text', :locals => {:anchor => 'issue_summary', :title => "Issue Summary", :field => 'issue_summary'} %>

      <%= render :partial => 'reports/file_attachments' %>

      <%= yield :wizard_actions %>

    <% end %>
