<% testcase = testcase_history %>
<tr class="testcase">
  <td class="testcase_name"><%= testcase.name %></td>
  <% @history.reverse_each do |h| %>
    <% tc = testcase.find_matching_case(h)  %>
    <% if tc %>
      <td class="testcase_result history_result <%=result_class(tc)%>"><%=result_html(tc)%></td>
    <% else %>
      <td class="testcase_result history_result">-</td>
    <% end %>
  <% end %>
  <td class="testcase_result <%= result_class(testcase) %> current_result"><%=result_html(testcase)%></td> 		
  <td></td>		

</tr>

