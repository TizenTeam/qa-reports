<tr class="feature_name">
  <td colspan="<%= 2 + 2*@comparison.columns.length %>"><%=group.name%><a href="#" class="see_all_toggle">+ see all</a></td>
</tr>
</tbody>
<tbody>
    <% group.rows.each{|row| %>
        <tr class="testcase<%= " has_changes" if row.changed %>" style="<%= 'display:none;' if !row.changed%>">
            <td char="testcase_name"><%=row.name%></td>
            <% @comparison.columns.each{|column|
                testcase = row.value(column).left
            %>
                <td class="testcase_result <%=result_class(testcase)%>"><%=result_html(testcase)%></td>
            <% } %>
            <% @comparison.columns.each_with_index{|column, index|
                testcase = row.value(column).right
            %>
            <td class="testcase_result <%=result_class(testcase)%>">
              <% if index==0 %><span class="<%= (row.changed ? 'changed_result' : 'unchanged_result') %>"><% end %>
              <%=result_html(testcase)%>
              <% if index==0 %></span><% end %>
            </td>
            <% } %>
        </tr>
    <%} %>
</tbody>
