<head>
<meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />
<meta http-equiv="X-UA-Compatible" content="IE=8" />
<title>MeeGo QA Reports</title>
<%= stylesheet_link_tag :all %>
<% if use_nokia_layout? %>
  <link href="/stylesheets/themes/nokia.css" rel="stylesheet" type="text/css" media="all">
<% end %>
<%= javascript_include_tag "jquery.min.js"%>
<%= javascript_include_tag "jquery-ui.min.js"%>
<%= javascript_include_tag 'bluff/js-class.js'%>
<%= javascript_include_tag 'bluff/excanvas.js'%>
<%= javascript_include_tag 'bluff/bluff-min.js'%>
<%= javascript_include_tag 'application'%>
<%= csrf_meta_tag %>
<% if @show_rss %>
<link rel="alternate" type="application/rss+xml" title="RSS" 
      href="<%=rss_url%>"/>
<% end %>
<%= google_analytics_tag %>
<%= clicktale_top_tag %>
</head>
<body>
<a name="top"></a>

  <%= render :partial => "reports/page_header" %>
  <%= yield :version_navi %>
  
  <div id="page">

	<%= yield %>

  </div>
  <div id="footer">
  	
  </div>
</div>

<%= clicktale_bottom_tag %>
</body>
</html>
